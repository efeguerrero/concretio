---
//Astro Component Imports
import Layout from '../layouts/Layout.astro';
import Hero from '../components/hero.astro';
import About from '../components/about.astro';
import Invertir from '../components/invertir/invertir.astro';
import Cta from '../components/cta.astro';
import Proyectos from '../components/proyectos/proyectos.astro';
import Equipo from '../components/equipo.astro';
import Footer from '../components/footer.astro';
import Contacto from '../components/contacto/contacto.astro';

//React Components Imports
---

<Layout title="Welcome to Astro.">
  <main>
    <Hero />
    <About />
    <Invertir />
    <Cta />
    <Proyectos />
    <Equipo />
    <Contacto client:visible />
    <Footer />
  </main>
</Layout>
